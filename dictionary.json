{
	"html": [{
		"xmlns": "http://www.w3.org/1999/xhtml",
		"xmlns:h": "http://xmlns.jcp.org/jsf/html"
	}],
	"head": "h:head",
	"/head": "/h:head",
	"body": "h:body",
	"/body": "/h:body",
	"textarea": "h:inputTextarea",
	"/textarea": "/h:inputTextarea",
	"button": [{
		"button": "h:commandButton"
	}],
	"img": [{
		"img": "h:graphicImage",
		"src": "value"
	}],
	"input": [{
		"type": [{
			"hidden": "h:inputHidden",
			"checkbox": "h:selectBooleanCheckbox",
			"password": "h:inputSecret",
			"radio": "h:selectOneRadio",
			"text": "h:inputText",
			"submit": "h:commandButton"
		}]
	}],
	"a": [{
		"a": "h:outputLink",
		"href": "value"
	}],
	"/a": "/h:outputLink",
	"select": "h:selectOneMenu",
	"/select": "/h:selectOneMenu",
	"option": [{
		"option": "f:selectItem",
		"value": "itemValue"
	}],
	"radio": [{
		"radio": "f:selectItem",
		"value": "itemValue",
		"label": "itemLabel",
		"select": "h:selectOneRadio",
		"/select": "/h:selectOneRadio"
	}],
	"script": [{
		"script": "h:outputScript",
		"type": "library",
		"src": "name"
	}],
	"link": [{
		"link": "h:outputStylesheet",
		"type": "library",
		"href": "name"
	}],
	"label": "h:outputLabel",
	"/label": "/h:outputLabel",
	"span": "h:outputText",
	"/span": "/h:outputText",
	"form": "h:form",
	"/form": "/h:form",
	"div": "h:panelGroup",
	"/div": "/h:panelGroup",
	"table": "h:panelGrid",
	"/table": "/h:panelGrid",
	"thead": "f:facet",
	"/thead": "/f:facet",
	"tfoot": "f:facet",
	"/tfoot": "/f:facet"
}
